{
  "name": "test-paypal",
  "version": "1.0.0",
  "description": "integration paypal server",
  "main": "index.js",
  "author": "YO-G <yonath-g@hotmail.fr>",
  "license": "MIT",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "build": "cd client && npm run build",
    "start": "node server",
    "start:client": "cd client && npm start",
    "start:server": "cd server && npm start",
    "start:dev": "npm-run-all -p start:client start:server",
    "install:dev": "npm i --legacy-peer-deps && cd client && npm i --legacy-peer-deps && cd ../server && npm i --legacy-peer-deps",
    "postinstall": "cd server && npm i"
  },
  "dependencies": {
    "cookie-parser": "^1.4.5"
  }
}
